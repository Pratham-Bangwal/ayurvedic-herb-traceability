  "express-rate-limit": "^7.0.0",
  "helmet": "^7.1.0",
    "ipfs-http-client": "^60.0.1",
    "mongoose": "^7.3.1",
    "multer": "^1.4.5-lts.1",
    "pino": "^8.21.0",
    "pino-pretty": "^11.2.2",
    "qrcode": "^1.5.1",
  "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.6.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2",
    "supertest": "^6.3.3"
